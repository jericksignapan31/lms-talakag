{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.json",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "icons",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/icon-*.png"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "/api/**",
        "https://firebasestorage.googleapis.com/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxAge": "1d",
        "maxSize": 100
      }
    },
    {
      "name": "api-freshness",
      "urls": [
        "https://**.firebaseio.com/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "10m",
        "maxSize": 50
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/**/*.*"
  ]
}
